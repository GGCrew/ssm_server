<%- if false -%>
<ul class="menu_bar">
	<li>Pending</li>
	<li>Approved</li>
	<li>Denied</li>
</ul>

<div class="clear_float"></div>
<%- end -%>

<div class="photo_section">
	<div class="sidebar">
		<div class="buttons">
			<%= link_to('Approve', '#', class: 'approve') -%></li>
			<%= link_to('Deny', '#', class: 'deny') -%></li>
		</div>
				
		<div class="filenames">
			<%- @photos.each do |photo| -%>
				<%= link_to(format_filename(photo).html_safe, photo_path(photo.id)) -%><br>
			<%- end -%>
		</div>
	</div>
	
	<div class="preview">
		<img src="<%= Photo::RESIZED_FOLDER + @photos.first.path -%>">
		<br>
		<p class="caption"><%= @photos.first.path -%></p>
	</div>
	
	<div class="clear_float">
	</div>
</div>
